        "compile": "tsc",
        "postcompile": "npm run build:cjs",
        "build:cjs": "rollup -c",
        "prepare": "npm run compile"
      },
      "repository": {
        "type": "git",
        "url": "git+https://github.com/yargs/cliui.git"
      },
      "standard": {
        "ignore": [
          "**/example/**"
        ],
        "globals": [
          "it"
        ]
