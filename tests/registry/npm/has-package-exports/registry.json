      "name": "has-package-exports",
      "version": "1.3.0",
      "description": "Does the current node version have support for the \"exports\" field in package.json?",
      "main": "no",
      "browser": {
        "no": "./browser.js",
        "yes": "./browser.js"
      },
      "exports": {
        ".": [
          {
            "browser": "./browser.js",
            "default": "./yes.js"
          },
          "./yes.js"
        ],
        "./conditional": [
          {
            "browser": "./browser.js",
            "import": "./yes.js",
            "require": "./yes.js",
            "node": "./yes.js",
            "default": "./no.js"
          },
          "./no.js"
        ],
        "./pattern": "./pattern.js",
        "./pattern-trailers": "./pattern-trailers.js"
      },
