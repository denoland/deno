{
  "tests": {
    "no_sloppy": {
      "args": "run --check main.ts",
      "output": "no_sloppy.out",
      "exitCode": 1
    },
    "sloppy": {
      "args": "run --unstable-sloppy-imports --check main.ts",
      "output": "sloppy.out"
    },
    "sloppy_env": {
      "args": "run --env-file=env_file --check main.ts",
      "output": "sloppy.out"
    }
  }
}
