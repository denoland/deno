{
  "tests": {
    "module_resolution_bundler": {
      "args": "check --quiet",
      "output": "main.out",
      "exitCode": 1
    },
    "module_esnext": {
      "args": "check -c deno_module_esnext.jsonc --quiet",
      "output": "main.out",
      "exitCode": 1
    },
    "module_preserve": {
      "args": "check -c deno_module_preserve.jsonc --quiet",
      "output": "main.out",
      "exitCode": 1
    }
  },
  "variants": {
    "tsgo": {
      "use_tsgo": "1"
    },
    "tsc": {
      "use_tsgo": ""
    }
  },
  "envs": {
    "DENO_UNSTABLE_TSGO": "${use_tsgo}"
  }
}
