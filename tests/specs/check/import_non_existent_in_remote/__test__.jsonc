{
  "tests": {
    "not_all": {
      "args": "check --allow-import import_remote.ts",
      "output": "[WILDCARD]",
      "exitCode": 0
    },
    "all": {
      "args": "check --all --allow-import import_remote.ts",
      "output": "check_all.out",
      "exitCode": 1
    }
  },
  "variants": {
    "tsgo": {
      "use_tsgo": "1"
    },
    "tsc": {
      "use_tsgo": ""
    }
  },
  "envs": {
    "DENO_UNSTABLE_TSGO": "${use_tsgo}"
  }
}
