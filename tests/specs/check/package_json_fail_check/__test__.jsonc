{
  "tempDir": true,
  "steps": [
    {
      "args": "install",
      "output": "install.out"
    },
    {
      "args": "check --quiet fail_check.ts",
      "output": "fail_check.out",
      "exitCode": 1
    }
  ],
  "variants": {
    "tsgo": {
      "use_tsgo": "1"
    },
    "tsc": {
      "use_tsgo": ""
    }
  },
  "envs": {
    "DENO_UNSTABLE_TSGO": "${use_tsgo}"
  }
}
