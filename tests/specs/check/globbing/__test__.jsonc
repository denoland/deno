{
  "tests": {
    "star": {
      "args": "check *.ts",
      "output": "Check [WILDCARD]main.ts\n",
      "exitCode": 0
    },
    "star_not_found": {
      "args": "check *.js",
      "output": "Warning No matching files found.\n",
      "exitCode": 0
    },
    "glob_star": {
      "args": "check **/*.ts",
      "output": "Check [WILDCARD]main.ts\nTS2322[WILDCARD]",
      "exitCode": 1
    },
    "sub_dir": {
      "args": "check sub_dir",
      "output": "Check [WILDCARD]main.ts\nTS2322[WILDCARD]",
      "exitCode": 1
    }
  },
  "variants": {
    "tsgo": {
      "use_tsgo": "1"
    },
    "tsc": {
      "use_tsgo": ""
    }
  },
  "envs": {
    "DENO_UNSTABLE_TSGO": "${use_tsgo}"
  }
}
