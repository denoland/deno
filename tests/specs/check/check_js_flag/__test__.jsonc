{
  "tests": {
    "without_flag": {
      "args": "check main.js",
      "output": "without_flag.out",
      "exitCode": 0
    },
    "with_flag": {
      "args": "check --check-js main.js",
      "output": "with_flag.out",
      "exitCode": 1
    }
  },
  "variants": {
    "tsgo": {
      "use_tsgo": "1"
    },
    "tsc": {
      "use_tsgo": ""
    }
  },
  "envs": {
    "DENO_UNSTABLE_TSGO": "${use_tsgo}"
  }
}
