{
  "envs": {
    "JSR_URL": "https://jsr.io/"
  },
  "tempDir": true,
  "tests": {
    "jsr": {
      "steps": [
        {
          "args": "add path@https://deno.land/std@1.1.0/path/mod.ts",
          "exitCode": 0,
          "output": "jsr_alias_add.out"
        },
        {
          "args": [
            "eval",
            "console.log(Deno.readTextFileSync('deno.json').trim())"
          ],
          "output": "jsr_deno.json.out"
        }
      ]
    },
    "npm": {
      "steps": [
        {
          "args": "add chalk_alias@https://esm.sh/chalk@5.4.1",
          "exitCode": 0,
          "output": "npm_alias_add.out"
        },
        {
          "args": [
            "eval",
            "console.log(Deno.readTextFileSync('deno.json').trim())"
          ],
          "output": "npm_deno.json.out"
        }
      ]
    }
  }
}
