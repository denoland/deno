{
  "tempDir": true,
  "steps": [{
    "args": "install",
    "output": "[WILDCARD]"
  }, {
    "if": "notMacIntel",
    "args": "compile --output main main.js",
    "output": "compile.out"
  }, {
    "if": "notMacIntel",
    "commandName": "./main",
    "args": [],
    "output": "output.out",
    "exitCode": 0
  }, {
    "if": "windows",
    "args": "compile --output main.exe main.js",
    "output": "compile.out"
  }, {
    "if": "windows",
    "commandName": "./main.exe",
    "args": [],
    "output": "output.out",
    "exitCode": 0
  }]
}
